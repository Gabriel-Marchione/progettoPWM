<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProgettoPWM"
        tools:targetApi="31">
        <activity
            android:name="com.progettopwm.progettopwm.BenvenutoActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.progettopwm.progettopwm.autenticazioneUtente.LoginActivity">
        </activity>
        <activity
            android:name="com.progettopwm.progettopwm.profiloUtente.ProfiloUtenteActivity">
        </activity>

        <activity
            android:name="com.progettopwm.progettopwm.registrazioneUtente.RegistrazioneUtenteActivity">
        </activity>
    </application>

</manifest>